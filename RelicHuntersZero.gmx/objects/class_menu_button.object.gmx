        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Use and Selection

if ( global.input == 'keyboard' )
{
    if (is_mouse_over(id))
    {
        global.selectedButton = id;
    }
    else if (global.selectedButton == id) global.selectedButton = -1;
}

if ( global.selectedButton == id )
{
    isSelected = true;
}
else
{
    isSelected = false;
}

if (isSelected) &amp;&amp; (isAvailable)
{
    if ((global.input == 'keyboard') &amp;&amp; mouse_check_button_pressed(mb_left) ) || ((global.input == 'joystick') &amp;&amp; joy_check(0,1))
    {
        executeAction = true;
        audio_play_sound(sfx_menu_click,1,false);
    }
}

// Auto-use with shortcut key pressed
var checkShortcut = false;

if (keyboardShortcut != -1) if (keyboard_check_pressed(keyboardShortcut)) checkShortcut = true;
if (joystickShortcut != -1) if (joy_check_pressed(0,joystickShortcut)) checkShortcut = true;

if (checkShortcut) &amp;&amp; (isAvailable)
{
    checkShortcut = false;
    global.selectedButton = id;
    isSelected = true;
    executeAction = true;
    audio_play_sound(sfx_menu_click,1,false);
}

// Star Movement &amp; Audio
if (isSelected)
{
    if (!audioSelectionPlayed)
    {
        audio_play_sound(sfx_menu_mouse,1,false);
        audioSelectionPlayed = true;
    }
    if (starScale &gt; starScaleFinal) starScale -= min( starScaleSpeed, abs(starScale-starScaleFinal) );
}
else
{
    audioSelectionPlayed = false;
    starScale = starScaleInitial;
}
</string>
